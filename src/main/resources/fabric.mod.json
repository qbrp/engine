{
  "schemaVersion": 1,
  "id": "engine",
  "version": "${version}",
  "name": "Engine",
  "description": "Модификация сервера qbrp, добавляет новые механики и утилиты для РП",
  "authors": ["Lain1wakura"],
  "contact": {
    "homepage": "https://wiki.qbrp.online"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/engine/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "org.lain.engine.client.MinecraftEngineClient"
    ],
    "server": [
      "org.lain.engine.DedicatedServerEngineMod"
    ],
    "main": [
      "org.lain.engine.CommonEngineServerMod"
    ]
  },
  "mixins": [
    "engine.mixins.json",
    {
      "config": "engine.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-language-kotlin": ">=${kotlin_loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
